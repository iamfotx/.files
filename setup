#!/usr/bin/env bash

set -e

uname=`uname`
platform="unknown"
if [[ $uname == "Darwin" ]]; then
   platform="macos"
elif [[ $uname == "Linux" ]]; then
   platform="linux"
fi

if [ "$platform" == "macos" ]; then
#   ./setup-homebrew
  ./setup-mac
fi

# ./setup-bash
# ./setup-git
# ./setup-vim
# ./setup-node